{
  "manifest_version": 3,
  "name": "Pick AI",
  "version": "0.1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": { "default_popup": "popup.html" },
  "background": { "service_worker": "background.js" },
  "commands": {
    "solve": {
      "suggested_key": {
        "mac": "Command+Shift+Z",
        "default": "Ctrl+Shift+Z"
      },
      "description": "Solve question"
    }
  }
}
